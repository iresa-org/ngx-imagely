{
  "name": "ngx-imagely-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --base-href https://iresa-org.github.io/ngx-imagely/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "release:first": "npm run release -- --first-release",
    "build:lib": "ng build --prod @iresa/ngx-imagely && npm run schematics:build && npm run copy-readme",
    "test:lib": "ng test @iresa/ngx-imagely",
    "release": "cd projects/iresa/ngx-imagely && standard-version --infile ../../../CHANGELOG.md",
    "test:lib:headless": "cross-env CI=true npm run test:lib",
    "publish:lib": "npm publish dist/iresa/ngx-imagely",
    "copy-readme": "copyfiles README.md dist/iresa/ngx-imagely",
    "schematics:build": "tsc -p tsconfig.schematics.json && npm run schematics:copy",
    "schematics:copy": "copyfiles schematics/collection.json dist/iresa/ngx-imagely && copyfiles schematics/ng-add/schema.json dist/iresa/ngx-imagely"
  },
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-free": "^5.13.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@ngneat/lib": "^1.0.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.2",
    "@angular-devkit/build-ng-packagr": "~0.1000.2",
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@angular/language-service": "~10.0.3",
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-angular": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@ngneat/spectator": "^5.11.0",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "all-contributors-cli": "^6.16.1",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "cross-env": "^7.0.2",
    "git-cz": "^4.7.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^10.2.11",
    "ng-packagr": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "standard-version": "^8.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run hooks:pre-commit && lint-staged",
      "pre-push": "npm run test:lib:headless"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
